generator client {
   provider = "prisma-client-js"
}

datasource db {
   provider = "postgresql"
   url      = env("DATABASE_URL")
}

model Users {
   documentId Int      @id @unique
   names      String
   email      String   @unique
   phone      String   @unique
   password   String
   roles      String[]
   isActive   Boolean  @default(true)
   createdAt  DateTime
   updatedAt  DateTime
}

model Clients {
   documentId Int      @id @unique
   name       String
   isActive   Boolean  @default(true)
   createdAt  DateTime
   updatedAt  DateTime
}

model Base {
   id         Int      @id @unique
   operation  Int
   bank       Int
   account    Int
   expiration String
   disburse   Float
   office     String
   dependence String
   isActive   Boolean
   createdAt  DateTime
   updatedAt  DateTime
}

model Assignment {
   id         Int      @id @unique
   operation  Int
   bank       Int
   assignment String
   createdAt  DateTime
   updatedAt  DateTime
}

model Campaign {
   id        Int      @id @unique
   operation Int
   bank      Int
   campaign  String
   createdAt DateTime
   updatedAt DateTime
}

model General {
   id         Int      @id @unique
   operation  Int
   bank       Int
   state      String
   substate   String
   assignment String
   createdAt  DateTime
   updatedAt  DateTime
}

model Wallet {
   id        Int      @id @unique
   operation Int
   bank      Int
   wallet    String
   createdAt DateTime
   updatedAt DateTime
}

model Decil {
   id        Int      @id @unique
   operation Int
   bank      Int
   decil     Int
   createdAt DateTime
   updatedAt DateTime
}

model Balance {
   id        Int      @id @unique
   operation Int
   bank      Int
   capital   Float
   TOTAL     Float
   createdAt DateTime
   updatedAt DateTime
}

model Emails {
   id          Int      @id @default(autoincrement())
   documentId  Int
   email       String
   description String
   isActive    Boolean
   createdAt   DateTime
   updatedAt   DateTime
}

model Phones {
   id          Int      @id @default(autoincrement())
   documentId  Int
   phone       String
   type        String
   description String
   isActive    Boolean
   createdAt   DateTime
   updatedAt   DateTime
}

model Payment {
   id         Int      @id @default(autoincrement())
   operation  Int
   bank       Int
   value      Int
   assignment String
   date       DateTime
   createdAt  DateTime
}

model Management {
   id          Int      @id @default(autoincrement())
   operation   Int
   bank        Int
   assignment  String
   type        String
   destiny     String
   detail      String
   channel     String
   management  String
   createStart DateTime
   createEnd   DateTime
   contrast    String
   updatedAt   DateTime
}

model Eps {
   id         Int      @id @default(autoincrement())
   documentId Int
   name       String
   type       String
   detail     String
   assignment String
   createdAt  DateTime
   updatedAt  DateTime
}

model Properties {
   id           Int      @id @default(autoincrement())
   documentId   Int
   property     String
   enrollment   String
   registration String
   city         String
   direction    String
   detail       String
   assignment   String
   createdAt    DateTime
   updatedAt    DateTime
}

model Employment {
   id           Int      @id @default(autoincrement())
   documentId   Int
   company      String
   documentType String
   document     String
   salary       String
   city         String
   direction    String
   phone        String
   email        String
   detail       String
   assignment   String
   createdAt    DateTime
   updatedAt    DateTime
}

